/*清除默认样式*/
*{ 
  padding:0; 
  margin:0; 
}
li{ 
  list-style:none; 
} 
a{ 
  text-decoration:none; 
  color:black;
}
.fl{ 
  float:left; 
}
.fr{ 
  float:right; 
}
.clearFix:after{ 
  content:""; display:block; clear:both; 
}
.clearFix{ 
  zoom:1; 
}
img,input,textarea,select{ 
  vertical-align:top; 
}
input,textarea,select{ 
  outline:none; 
  border:none;
}
textarea{ 
  resize:none; 
}
html{
  font-size:32px;
  font-family: 微软雅黑;
}
.wrapper{
  width:10rem;
  margin:0 auto;
}
html,body{
  height:100%;
}
/*清除默认样式结束*/
