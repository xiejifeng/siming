html {
    font-size: 100px;
}
body {
    background: #f5f5f5;
    font-size: 0.13rem;
}
ul {
    list-style-type: none;
}
.dis-none {
    display: none!important;
}
.f-l {
    float: left!important;
}
a:focus,
input:focus,
select:focus,
button:focus,
textarea:focus {
    outline: none;
}
* {
    margin: 0;
    padding: 0;
    font: 0.12rem;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
@color-default: #feb801;
@color-default-active: #c99102;
@color-light: #fccb4e;
@color-font: #3b3b3b;
@color-font-text: #979696;
.title-box {
    width: 100%;
    height: 0.44rem;
    background: @color-default;
    .title {
        color: @color-font;
        text-align: center;
        font-size: 0.14rem;
        line-height: 0.44rem;
    }
    .g-left-icon {
        height: 0.44rem;
        width: 0.6rem;
        padding: 0.08rem 0.13rem;
        position: absolute;
        left: 0;
        top: 0;
        img {
            position: absolute;
            left: 0.13rem;
            top: 0.17rem;
            height: 0.23rem;
        }
    }
    .g-left-icon.active {
        background: #eee;
        border-bottom: 1px solid @color-default;
    }
    .g-right-icon{
        width: 0.8rem;
        height: 0.44rem;
        padding: 0.09rem;
        position: absolute;
        right: 0;
        top: 0;
        .sign-btn {
            width: 0.61rem;
            height: 0.26rem;
            line-height: 0.26rem;
            text-align: center;
            border: none;
            background: none;
            position: absolute;
            top: 0.09rem;
            right: 0.09rem;
            color: @color-default;
            font-size: 0.13rem;
            text-align: right;
            padding-right: 0.1rem;
            img{
                width: 0.26rem;
                height: 0.26rem;
            }
        }
    }
}
.g-banner-box {
    width: 100%;
    height: 1.33rem;
    position: relative;
    background: @color-default;
    overflow: hidden;
    ul {
        list-style: none;
        margin: 0;
        padding: 0;
        height: 100%;
        overflow: hidden;
        li {
          position: absolute;
        }
    }
    .banner-list {
        width: 100%;
        height: 1.6rem;
        li {
            width: 100%;
            height: 1.6rem;
            overflow: hidden;
            img {
                width: 100%;
            }
        }
    }
}
.g-entrance-box {
    width: 100%;
    height: 0.8rem;
    overflow: hidden;
    background: @color-default;
    ul{
        display: -webkit-box;
    }
    .g-entrance {
        -webkit-box-flex: 1;
        height: 0.5rem;
        margin: 0.15rem;
        background: @color-default;
        display: inline-block;
        float: left;
        position: relative;
        .icon {
            width: 0.34rem;
            height: 0.34rem;
            position: absolute;
            top: 0rem;
            left: 0.06rem;
        }
        .icon-title {
            color: #fff;
            text-align: center;
            font-size: 0.12rem;
            width: 0.5rem;
            position: absolute;
            left: 0;
            bottom: 0.03rem;
        }
    }
    li.active {
        opacity: 0.5;
    }
}
.g-curriculumlist-box {
    width: 100%;
    height: auto;
    margin-top: 0.1rem;
    background: #fff;
    .g-list-title-box {
        width: 100%;
        height: 0.39rem;
        border-top: 1px solid #eee;
        h2 {
            width: 100%;
            padding-left: 0.2rem;
            line-height: 0.4rem;
            color: @color-font;
            text-align: left;
            font-size: 0.16rem;
            font-weight: normal;
            span {
                line-height: 0.4rem;
                font-size: 0.12rem;
                padding: 0 0.2rem;
                color: @color-default;
                float: right;
                text-decoration: underline;
            }
        }
    }
    .g-list-box {
        width: 100%;
        height: auto;
        padding: 0 0.2rem 0.2rem;
        li {
            width: 100%;
            height: 0.9rem;
            border-bottom: 1px solid #eee;
            padding: 0.1rem 0;
            transition: all 0.2s ease-in-out;
            -moz-transition: all 0.2s ease-in-out;
            -webkit-transition: all 0.2s ease-in-out;
            -o-transition: all 0.2s ease-in-out;
            .g-curriculumlist-img-box {
                width: 35%;
                height: 0.7rem;
                padding-right: 0.1rem;
                display: inline-block;
                float: left;
                position: relative;
                img {
                    width: 92%;
                    height: 0.7rem;
                    position: absolute;
                    left: 0;
                    top: 0;
                }
            }
            .g-curriculumlist-info-box {
                width: 65%;
                height: 0.7rem;
                display: inline-block;
                float: left;
                position: relative;
                span {
                    font-size: 0.12rem;
                    position: absolute;
                }
                .cur-name {
                    font-size: 0.14rem;
                    top: 0.05rem;
                    left: 0;
                    width: 1.05rem;
                    white-space: nowrap;
                    overflow: hidden;
                    text-overflow: ellipsis;
                }
                .cur-tec {
                    top: 0.23rem;
                    left: 0;
                    color: @color-font-text;
                }
                .cur-date {
                    bottom: 0.05rem;
                    left: 0;
                    color: @color-font-text;
                }
                .cur-tatus {
                    top: 0.05rem;
                    right: 0;
                    color: @color-default;
                }
            }
        }
        li.active {
            background: #eee;
            width: 100%;
            margin-left: -0.2rem;
            padding: 0.1rem 0.2rem;
        }
    }
}
.g-form-group-box {
    width: 100%;
    padding-top: 0rem;
    margin-bottom: 0.6rem;
    .about-school-box{
        width: 100%;
        background: #fff;
        padding: 0.2rem;
        line-height: 0.24rem;
        font-size: 0.12rem;
        
        ul{
            border-top: 1px solid #ddd;
            margin-top: 0.1rem;
            li{
                border-bottom: 1px solid #ddd;
                width: 100%;
                padding: 0.1rem 0;
                font-size: 0.12rem;
            }
            span:first-child{
                display: inline-block;
                width: 0.7rem;
                vertical-align: middle;
            }
            span:last-child{
                display: inline-block;
                width: 2.05rem;
                vertical-align: middle;
            }
        }
        .small-text{
            padding: 0.05rem 0 0.05rem 0;
        }
    }
    .g-form-group {
        width: 100%;
        background: #fff;
        .form-list {
            width: 100%;
            height: 0.6rem;
            padding: 0.15rem 0.2rem;
            margin-bottom: 0.06rem;
            position: relative;
            label {
                width: 26%;
                text-align: right;
                display: inline-block;
                font-size: 0.13rem;
                line-height: 0.14rem;
                margin: 0.08rem 0;
                color: @color-font;
                float: left;
                border-left: 4px solid @color-light;
            }
            input,
            .g-select {
                width: 74%;
                height: 0.3rem;
                border-radius: 2px;
                border: 1px solid #bbb;
                padding: 0.07rem 0.1rem;
                line-height: 0.16rem;
                float: right;
                font-size: 0.12rem;
            }
            .g-input-btn-box {
                width: 74%;
                height: 0.3rem;
                display: inline-block;
                float: right;
                input {
                    width: 62%;
                    height: 0.3rem;
                    border-radius: 2px;
                    border: 1px solid #bbb;
                    padding-left: 0.1rem;
                    line-height: 0.3rem;
                    float: left;
                    font-size: 0.12rem;
                }
                button {
                    width: 35%;
                    height: 0.3rem;
                    border-radius: 2px;
                    border: none;
                    line-height: 0.3rem;
                    font-size: 0.12rem;
                    color: #fff;
                    background: @color-default;
                    float: right;
                }
            }
            .provision-box {
                border: none;
                text-align: left;
                width: 100%;
                font-size: 0.13rem;
                input {
                    width: 0.15rem;
                    height: 0.15rem;
                    float: left;
                }
                span {
                    float: left;
                    padding-left: 0.06rem;
                }
                a {
                    color: @color-default;
                    text-decoration: none;
                    padding-left: 0.06rem;
                }
            }
        }
        .form-box {
            width: 100%;
            padding: 0.15rem 0.2rem;
            margin-bottom: 0.06rem;
            position: relative;
            .agreement-box {
                width: 100%;
                height: 0.5rem;
                overflow: hidden;
                h2 {
                    font-size: 0.12rem;
                    line-height: 0.2rem;
                    margin: 0 0 0.1rem;
                };
                p {
                    font-size: 0.12rem;
                    line-height: 0.2rem;
                }
            }
            .agreement-box.active {
                height: 1.2rem;
                overflow-y: scroll;
            }
            .read-more {
                float: right;
                color: @color-default;
            }
        }
    }
}
.g-btn-bottom-box {
    width: 100%;
    height: 0.5rem;
    position: fixed;
    left: 0;
    bottom: 0;
    .sure-btn {
        width: 100%;
        height: 0.5rem;
        background: #fff;
        border: none;
        border-top: 1px solid #eee;
        color: @color-default;
        font-size: 0.18rem;
        position: absolute;
        left: 0;
        top: 0;
    }
    .sure-btn.active {
        background: @color-default;
        color: #fff!important;
    }
    .two-child{
        width: 1.6rem!important;
    }
    .two-child:last-child{
        left: auto;
        right: 0;
        border-left: 1px solid #eee;
        color: #979696;
    }
}
.g-login-box {
    background: @color-default;
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    .g-logo-box {
        width: 100%;
        height: 0.4rem;
        position: absolute;
        left: 0.2rem;
        top: 0;
        img {
            width: 100%;
            position: absolute;
            left: -0.2rem;
            top: 0rem;
        }
        h2 {
            line-height: 0.4rem;
            color: #fff;
            font-size: 0.24rem;
            padding-left: 0.5rem;
        }
    }
    .g-input-box {
        width: 100%;
        position: absolute;
        left: 0;
        top: 0.2rem;
        padding: 0 0.2rem;
        .submite-input {
            width: 88%;
            height: 0.4rem;
            border-radius: 2px;
            border: none;
            background: #fff;
            padding: 0.1rem 0.15rem;
            line-height: 0.2rem;
            color: @color-font;
            position: absolute;
            left: 6%;
            top: 1.0rem;
            font-size: 0.14rem;
        }
        .password {
            top: 1.6rem;
        }
        .g-yzm-box {
            width: 88%;
            height: 0.4rem;
            position: absolute;
            left: 6%;
            top: 2.2rem;
            .yzm-input {
                width: 55%;
                height: 0.4rem;
                border-radius: 2px;
                border: none;
                background: #fff;
                padding-left: 0.15rem;
                color: @color-font;
                float: left;
                font-size: 0.14rem;
            }
            .yzm {
                width: 40%;
                height: 0.4rem;
                float: right;
            }
        }
        .forget-password{
            font-size: 0.14rem;
            color: #fff;
            position: absolute;
            top: 2.05rem;
            text-decoration: none;
            right: 0.2rem;
        }
    }
    .g-submite-box {
        width: 100%;
        position: absolute;
        left: 0;
        top: 3.05rem;
        .submite-btn {
            width: 88%;
            height: 0.35rem;
            background: #fff;
            color: @color-default;
            font-size: 0.16rem;
            border-radius: 0.04rem;
            border: none;
            position: absolute;
            left: 6%;
            top: 0;
        }
        .sub-2{
            top: 0.45rem;
        }
        .sub-3{
            top: 0.9rem;
        }
        .submite-btn.active {
            background: @color-light;
        }
        a {
            font-size: 0.14rem;
            color: #fff;
            position: absolute;
            top: 1.35rem;
            text-decoration: none;
        }
        .forget-password {
            left: 0.2rem;
        }
        .register {
            right: 0.2rem;
        }
        a:hover {}
    }
}
.g-tab-box {
    width: 100%;
    height: 0.3rem;
    position: relative;
    background: #fff;
    display: -webkit-box;
    border-bottom: 1px solid @color-default;
    .tab-list {
        height: 0.3rem;
        -webkit-box-flex: 1;
        a {
            width: 100%;
            display: block;
            line-height: 0.3rem;
            color: @color-font;
            text-align: center;
            font-size: 0.13rem;
            text-decoration: none;
        }
        a.active {
            color: @color-default;
        }
    }
    .tab-list:visited{
        background: none;
    }
}
.btn.active {
    background: @color-default-active!important;
}
.tab2-box,
.tab3-box,
.tab4-box {
    display: none;
}
.g-user-box {
    width: 100%;
    height: 1rem;
    background: #fff;
    position: relative;
    border-bottom: 1px solid #eee;
    margin-bottom: 0.1rem;
    .g-user-img-box {
        width: 37.5%;
        height: 1rem;
        display: inline-block;
        float: left;
        position: relative;
        img {
            width: 0.56rem;
            height: 0.56rem;
            border-radius: 0.6rem;
            overflow: hidden;
            position: absolute;
            right: 0;
            top: 0.2rem;
        }
    }
    .g-user-message-box {
        width: 62.5%;
        height: 1rem;
        position: relative;
        display: inline-block;
        float: right;
        .user-name {
            font-size: 0.14rem;
            color: @color-font;
            position: absolute;
            left: 0.2rem;
            top: 0.3rem;
        }
        .user-status {
            font-size: 0.12rem;
            color: @color-default;
            position: absolute;
            top: 0.3rem;
            left: 1rem;
        }
        .user-integral {
            font-size: 0.12rem;
            color: @color-font-text;
            position: absolute;
            left: 0.2rem;
            top: 0.6rem;
        }
        .shutdown-icon {
            width: 0.2rem;
            height: 0.2rem;
            position: absolute;
            top: 0.1rem;
            right: 0.1rem;
        }
    }
}
.g-user-group {
    width: 100%;
    .user-list {
        width: 100%;
        height: 0.5rem;
        border-bottom: 1px solid #eee;
        position: relative;
        background: #fff;
        transition: all 0.2s ease-in-out;
        -moz-transition: all 0.2s ease-in-out;
        -webkit-transition: all 0.2s ease-in-out;
        -o-transition: all 0.2s ease-in-out;
        .icon-img {
            width: 0.3rem;
            height: 0.3rem;
            position: absolute;
            top: 0.1rem;
            left: 0.2rem;
        }
        .user-title {
            font-size: 0.13rem;
            line-height: 0.3rem;
            position: absolute;
            top: 0.1rem;
            left: 0.7rem;
        }
        .right-arrow {
            width: 0.16rem;
            height: 0.16rem;
            position: absolute;
            right: 0.2rem;
            top: 0.18rem;
        }
    }
    .user-list.active {
        background: #eee;
    }
    .user-list:nth-child(3),
    .user-list:nth-child(6) {
        margin-bottom: 0.1rem;
    }
}
.g-integral-group {
    width: 100%;
    .integral-list {
        width: 88%;
        height: 0.7rem;
        background: #fff;
        border: 1px solid #eee;
        margin: 0.1rem 6%;
        position: relative;
        transition: all 0.2s ease-in-out;
        -moz-transition: all 0.2s ease-in-out;
        -webkit-transition: all 0.2s ease-in-out;
        -o-transition: all 0.2s ease-in-out;
        .integral-message {
            font-size: 0.16rem;
            line-height: 0.7rem;
            display: block;
            text-align: center;
            .red {
                color: @color-default;
                padding: 0 0.1rem;
            }
        }
        .label-top {
            height: 0.32rem;
            position: absolute;
            right: 0.08rem;
            top: -0.05rem;
        }
    }
    .integral-list.active {
        background: #eee;
    }
}
.g-subject-box {
    width: 100%;
    height: 4rem;
    background: #fff;
    border-bottom: 1px solid #eee;
    .g-subject-level-box {
        width: 43.75%;
        height: 1.4rem;
        margin: 0 28.125% 0.3rem;
        background: url("../imgs/level.png") no-repeat;
        background-size: 100%;
        position: relative;
        .level {
            font-size: 0.2rem;
            color: @color-font;
            display: block;
            width: 100%;
            text-align: center;
            position: absolute;
            left: 0;
            top: 0.6rem;
        }
    }
    .g-subject-message-box {
        margin-top: 0.15rem;
        width: 100%;
        .subject-level {
            width: 100%;
            text-align: center;
            font-size: 0.32rem;
            color: @color-default;
            margin-bottom: 0.4rem;
        }
        .subject {
            font-size: 0.14rem;
            width: 100%;
            padding: 0.06rem 0.9rem;
            text-align: center;
            .red {
                color: @color-default;
            }
        }
    }
}
.sign-table {
    width: 100%;
    background: #fff;
    dt {
        font-size: 0.14rem;
        width: 100%;
        height: 0.6rem;
        border-bottom: 1px solid #eee;
        span {
            width: 33.33%;
            line-height: 0.6rem;
            display: inline-block;
            float: left;
            text-align: center;
            font-weight: bold;
        }
    }
    dd {
        font-size: 0.13rem;
        width: 100%;
        height: 0.5rem;
        border-bottom: 1px solid #f6f6f6;
        span {
            width: 33.33%;
            line-height: 0.5rem;
            display: inline-block;
            float: left;
            text-align: center;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
        }
        .sign-status {
            color: @color-default-active;
            font-size: 0.16rem;
            img {
                width: 0.28rem;
                vertical-align: middle;
            }
        }
        .integral-add {
            color: green;
        }
    }
}
.feedback-text {
    width: 100%;
    height: 2.2rem;
    border-radius: 2px;
    border: 1px solid #eee;
    padding: 0.2rem;
    line-height: 0.24rem;
    font-size: 0.13rem;
}
.about-school {
    width: 3rem;
    margin: 0 0.1rem 0.1rem;
}
.school-num {
    width: 100%;
    font-size: 0.13rem;
    color: @color-default;
    text-align: center;
    margin-top: 0.1rem;
}
.student-table {
    width: 100%;
    background: #fff;
    dt {
        font-size: 0.14rem;
        width: 100%;
        height: 0.6rem;
        border-bottom: 1px solid #eee;
        line-height: 0.6rem;
        span {
            display: inline-block;
            float: left;
            text-align: center;
            line-height: 0.6rem;
        }
    }
    dd {
        font-size: 0.13rem;
        width: 100%;
        height: 0.5rem;
        border-bottom: 1px solid #eee;
        line-height: 0.5rem;
        span {
            display: inline-block;
            float: left;
            text-align: center;
        }
    }
    span:nth-child(1) {
        width: 12.5%;
        height: 0.5rem;
    }
    span:nth-child(2) {
        width: 25%;
    }
    span:nth-child(3),
    span:nth-child(4) {
        width: 31.25%;
    }
}
.g-loading-bg-box {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100000;
    background: @color-default;
    .g-loading-box {
        width: 100%;
        position: absolute;
        top: 30%;
        left: 0;
        .loading-text {
            text-align: center;
            font-size: 0.15rem;
            margin-top: -26px;
            margin-bottom: 0;
        }
    }
}
.alert {
    width: 87.5%;
    height: 0.5rem;
    line-height: 0.5rem;
    text-align: center;
    font-size: 0.14rem;
    border-radius: 0.02rem;
    border: 1px solid #eee;
    box-shadow: 0 0 0.1rem #ddd;
    position: fixed;
    bottom: 0.6rem;
    left: 0.2rem;
}
.alert-success {
    color: #3c763d;
    background: #dff0d8;
    border-color: #d6e9c6;
}
.alert-info {
    color: #31708f;
    background: #d9edf7;
    border-color: #bce8f1;
}
.alert-warning {
    color: #8a6d3b;
    background: #fcf8e3;
    border-color: #faebcc;
}
.alert-danger {
    color: #a94442;
    background: #f2dede;
    border-color: #ebccd1;
}
.video-tab-box {
    .tab-list {
        width: 1.6rem;
        a {
            width: 1.6rem;
        }
    }
}
.g-sub-list-box {
    width: 100%;
    .sub-list {
        width: 100%;
        height: 0.5rem;
        border-bottom: 1px solid #eee;
        position: relative;
        background: #fff;
        margin-bottom: 0.1rem;
        transition: all 0.2s ease-in-out;
        -moz-transition: all 0.2s ease-in-out;
        -webkit-transition: all 0.2s ease-in-out;
        -o-transition: all 0.2s ease-in-out;
        .icon-sub {
            width: 0.3rem;
            height: 0.3rem;
            position: absolute;
            top: 0.1rem;
            left: 0.2rem;
            color: #fff;
            font-size: 0.12rem;
            text-align: center;
            line-height: 0.3rem;
            overflow: hidden;
        }
        .sub-title {
            font-size: 0.13rem;
            line-height: 0.3rem;
            position: absolute;
            top: 0.1rem;
            left: 0.7rem;
