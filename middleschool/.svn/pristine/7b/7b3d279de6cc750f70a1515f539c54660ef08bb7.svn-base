header {
	height: 0.93rem;
	background-color: #00acf0;
	text-align: center;
	padding-top: 0.3rem;
	position: relative;
}

h2 {
	font-size: 0.61rem;
	color: white;
	font-weight: normal;
	margin: 0;
}

.back {
	position: absolute;
	left: 0.29rem;
	color: white;
	bottom: 0.1rem;
	font-family: 宋体;
}

.back .back_btn {
	height: 0.8rem;
	width: auto;
}

.home {
	width: 0.86rem;
	height: 0.75rem;
	position: absolute;
	right: 0.33rem;
	bottom: 0.2rem;
	background: url(../images/home.png) no-repeat;
	background-size: 100% 100%;
}

footer.btn-pro {
	margin: 0 auto;
	width: 8.81rem;
	;
	background-color: #00acf0;
	height: 1.1rem;
	line-height: 1.1rem;
	text-align: center;
	font-size: 0.5rem;
	color: white;
	position: fixed;
	bottom: 1.25rem;
	border-radius: 0.18rem;
	left: 50%;
	margin-left: -4.40rem;
}

.wrap {
	width: 9.25rem;
	margin: 0 auto;
}

.banner {
	margin-top: 0.32rem;
}

.product {
	height: 5.42rem;
	text-align: center;
	overflow: hidden;
	background-color: #e6e6e6;
	/*line-height: 5.42rem;*/
	max-height: 5.42rem;
	text-align: center;
	margin-bottom: 0.57rem;
}
.sp-img{
	height: 100%;
}
.introduce h4 {
	font-weight: normal;
	font-size: 0.4rem;
}

.introduce p {
	font-size: 0.3rem;
	word-wrap: break-word;
	text-indent: 2em;
}

nav {
	overflow: hidden;
}

.tab-head {
	display: -webkit-box;
	background-color: #e6e6e6;
	padding-top: 0.43rem;
}

.tab-head li {
	-webkit-box-flex: 1;
	height: 1.07rem;
	text-align: center;
}

.tab-head a {
	display: block;
	width: 100%;
	height: 100%;
	line-height: 1.07rem;
	font-size: 0.55rem;
}

.tab-head img {
	width: 1.5rem;
}

.tab-head li.active {
	background: url(../images/tab-head-bg.png) no-repeat;
	background-size: 100% 100%;
}

.tab-body {
	width: 100%;
	padding: 0.36rem 0.49rem 0;
}

.tab-body a {
	display: block;
	width: 100%;
	height: 1.52rem;
	border-bottom: 1px solid #000;
	font-size: 0.4rem;
	line-height: 1.52rem;
	position: relative;
	white-space: nowrap;
}

.tab-body .day {
	margin-right: 0;
	display: inline-block;
	width: 4.9rem;
	height: 1.52rem;
	white-space: nowrap;
	overflow: hidden;
	table-layout:fixed;
	vertical-align: top;
}
.tab-body .time{
	display: inline-block;
	width: 2.5rem;
	height: 1.52rem;
	white-space: nowrap;
	overflow: hidden;
	table-layout:fixed;
	vertical-align: top;
}
.tab-body .icon {
	position: absolute;
	width: 0.72rem;
	height: 0.72rem;
	right: 0;
	top: 50%;
	margin-top: -0.36rem;
}

.tab-body li.correct .icon {
	background: url(../images/correct.png) no-repeat;
	background-size: 100% 100%;
}

.tab-body li.error .icon {
	background: url(../images/error.png) no-repeat;
	background-size: 100% 100%;
}

.container {
	width: 200%;
	/*display: -webkit-box;
	display: box;*/
	display: block;
	-webkit-transition: 1s all ease;
}

.container ul {
	/*-webkit-box-flex: 1;
	box-flex: 1;*/
	width: 9.02rem;
	float: left;
	-webkit-transform: translateY(0px);
	height: 13.5rem;
	overflow-y: scroll;
}

.bg-gray {
	background-color: #e6e6e6 !important;
}

section.link {
	margin-top: 0.42rem;
}

section.link li {
	margin-bottom: 0.42rem;
}

section.link a {
	display: block;
	width: 100%;
	height: 1.48rem;
	line-height: 1.48rem;
	background-color: #e6e6e6;
}

section.link .wrap {
	position: relative;
}

section.link p {
	position: absolute;
	left: 0;
	color: white;
	bottom: 0.1rem;
	font-family: 宋体;
	width: 1.5rem;
	height: 0.55rem;
	top: 0.46rem;
}

section.link b {
	position: absolute;
	right: 0;
	display: block;
	font-weight: normal;
	font-family: 宋体;
	color: black;
}

.wrap b>.next_btn {
	height: 0.8rem;
	margin-top: 0.3rem;
}

.xk-img-box img {
	width: 100%;
}

.bg-red {
	background-color: #ff7170 !important;
}

.user-call {
	height: 2.12rem;
	background-color: white;
	margin-top: 0.41rem;
	margin-bottom: 0.63rem;
	position: relative;
}

.user-call .user-dp {
	width: 1.46rem;
	height: 1.46rem;
	border: 0.03rem solid #000;
	border-radius: 50%;
	position: absolute;
	top: 50%;
	margin-top: -0.73rem;
	overflow: hidden;
}

.user-call p {
	font-size: 0.54rem;
	margin: 0;
}

.user-call span {
	display: block;
	font-size: 0.42rem;
	color: #e98b4e;
}

.user-call .user-info {
	margin-left: 1.85rem;
	position: absolute;
	top: 50%;
	margin-top: -0.5rem;
}

.nav-user {
	height: 9.04rem;
	background-color: white;
	border-radius: 0.4rem;
	padding: 1rem;
	-webkit-box-sizing: border-box;
}

.nav-user ul {
	height: 100%;
	position: relative;
	margin: 0;
	padding: 0;
}

.nav-user li {
	position: relative;
	width: 50%;
	height: 50%;
	-webkit-box-sizing: border-box;
}

.nav-user a {
	display: block;
	width: 1.5rem;
	height: 2.23rem;
	position: absolute;
}

.nav-user a div {
	width: 1.48rem;
	height: 1.48rem;
}

.nav-user a div img {
	width: 100%;
	height: 100%;
}

.nav-user a p {
	font-size: 0.36rem;
	text-align: center;
	margin-top: 0.25rem;
}

.nav-user .lt a {
	left: 0;
	top: 0;
}

.nav-user .lt {
	border-right: 1px solid #000;
	border-bottom: 1px solid #000;
}

.nav-user .lr a {
	right: 0;
	top: 0;
}

.nav-user .lr {
	border-left: 1px solid #000;
	border-bottom: 1px solid #000;
}

.nav-user .lb a {
	left: 0;
	bottom: 0;
}

.nav-user .lb {
	border-top: 1px solid #000;
	border-right: 1px solid #000;
}

.nav-user .rb a {
	right: 0;
	bottom: 0;
}

.nav-user .rb {
	border-left: 1px solid #000;
	border-top: 1px solid #000;
}

.nav-user .c {
	width: 4rem;
	height: 4rem;
	border: 1px solid #000;
	border-radius: 0.3rem;
	position: absolute;
	left: 50%;
	top: 50%;
	margin-left: -2rem;
	margin-top: -2rem;
	background-color: white;
	-webkit-transform: rotateZ(-45deg);
}

.nav-user .c a {
	left: 50%;
	top: 50%;
	margin-left: -0.74rem;
	margin-top: -1.12rem;
	-webkit-transform: rotateZ(45deg);
}

.in-block {
	display: inline-block;
}

.banner .title {
	font-size: 0.5rem;
	text-align: justify;
	overflow: hidden;
	/*height:0.75rem;*/
}


/*.banner .title>*{
	display: inline-block;
}*/

.banner .title span {
	font-size: 0.43rem;
	color: #ff6300;
}

.banner small,
.banner p {
	font-size: 0.35rem;
}

.context {
	/*margin-top: -0.5rem;*/
}

.list {
	margin-top: 0.32rem;
}

.list li {
	padding-bottom: 0.36rem;
	border-bottom: 1px solid #000;
	margin-bottom: 0.44rem;
}

.list a {
	display: block;
	width: 100%;
	height: 100%;
}

.list li .pic {
	border: 0.06rem solid #000;
    height: 1.4rem;
    width: 2.8rem;
    text-align: center;
    background: #eee;
}
.list li .pic img {
    height: 100%;
}
.list .info {
	font-size: 0.37rem;
	margin-top: 0.3rem;
}

.now-score {
	font-size: 0.58rem;
	text-align: right;
	position: absolute;
	bottom: 0.31rem;
}

.now-score span {
	color: #ff6300;
}

.video {
	height: 5.76rem;
	background-color: #ccc;
}

.record {
	height: 1.31rem;
	background-color: #00acf0;
}

.record a {
	display: block;
	width: 2.07rem;
	height: 0.83rem;
	background-color: white;
	border-radius: 0.15rem;
	margin-right: 0.26rem;
	margin-top: 0.23rem;
	font-size: 0.4rem;
	line-height: 0.83rem;
	text-align: center;
	color: #00acf0;
}

.chat {
	height: 1.4rem;
	background-color: #00acf0;
	position: fixed;
	bottom: 0;
	left: 0;
	padding: 0.27rem 0.22rem 0;
	-webkit-box-sizing: border-box;
}

.chat .chat-text {
	height: 0.86rem;
	width: 5.9rem;
	border-radius: 0.05rem;
	font-size: 0.55rem;
	text-indent: 0.5em;
}

.chat a {
	color: white;
	display: block;
	font-size: 0.55rem;
	width: 1.31rem;
	height: 0.86rem;
	text-align: center;
	line-height: 0.86rem;
}

.chat .send {
	margin-left: 0.43rem;
}

.go-on {
	text-align: center;
	font-size: 0.42rem;
	margin-top: 1.52rem;
}

.go-on div {
	width: 5.35rem;
	height: 2.58rem;
	display: inline-block;
	margin-bottom: 0.92rem;
	margin-left: -0.5rem;
}

.go-on .correct {
	width: 2.54rem;
	height: 2.54rem;
}

.go-on div img {
	width: 100%;
	height: 100%;
}

.go-on h3 {
	font-size: 0.55rem;
	font-family: 宋体;
	font-weight: 900;
	margin-bottom: 1.1rem;
}

.go-on p {
	line-height: 0.65rem;
}

.go-on b {
	font-weight: normal;
}

.go-on i {
	font-style: normal;
}

.go-top {
	width: 1.2rem;
	height: 1.2rem;
	background: url(../images/go-top.png) no-repeat;
	background-size: 100% 100%;
	background-color: white;
	position: fixed;
	right: 0rem;
	bottom: 1.65rem;
}

.pra-content {
	height: 5.42rem;
	background-color: #fff;
	line-height: 5.42rem;
	text-align: center;
	margin-bottom: 0.3rem;
	margin-top: 0.34rem;
}

.praxis i {
	font-style: normal;
	font-size: 0.5rem;
}

.praxis p {
	font-size: 0.5rem;
	word-wrap: break-word;
}
.praxis b {
	font-size: 0.57rem;
}

.praxis li {
	padding: 0 0.46rem;
	overflow: hidden;
	margin-bottom: 0.3rem;
}

.praxis dl {
	margin-top: -0.3rem;
}

.praxis dd {
	line-height: 0.9rem;
	position: relative;
	font-size: 0.5rem;
}

.praxis dd.sel div {
	width: 0.9rem;
	height: 0.9rem;
	position: absolute;
	background: url(../images/select.png) no-repeat;
	background-size: 100% 100%;
	left: -0.24rem;
	top: 0.12rem;
}

.question-return textarea {
	width: 100%;
	height: 9.23rem;
	border: 0.01rem solid #000;
	margin-top: 0.42rem;
	font-size: 0.5rem;
	overflow-y: scroll;
	padding: 0.3rem;
	-webkit-box-sizing: border-box;
}

.btn-wrap {
	position: fixed;
	/*height: 2.65rem;*/
	height: 1.75rem;
	left: 0;
	bottom: 0;
	background-color: #fff;
	text-align: center;
	display: -webkit-box;
}

.btn-pro {
	background-color: #00acf0;
	height: 1.1rem;
	line-height: 1.1rem;
	border: none;
	font-size: 0.6rem;
	color: white;
	border-radius: 5px!important;
	-webkit-box-flex: 1;
	margin: 0 0.6rem;
	margin-top: 0.15rem;
	display: inline-block;
	width: 8.8rem;
}

.list .wrap {
	height: 14.5rem;
	overflow-y: scroll;
}

.clearFix a {
	height: auto;
}

.answer{
	line-height: 1.1rem;
    position: relative;
    font-size: 0.5rem;
    display: block;
    margin: 0.2rem 0;
}
.answer>input{
	vertical-align: middle;
}
.answer>p{
	display: inline;
}
.answer.sel>p:last-child{
	width: 100%;
    height: 0.9rem;
    position: absolute;
    background: url(../images/select.png) no-repeat;
    left: -0.24rem;
    top: 0rem;
    padding-left: 0.9rem;
    background-size: 9%;
}
.pra-content.wrap{
	height: auto!important;
	line-height: 0!important;
	padding-top: 0.5rem;
}
.pra-content.wrap>p{
	text-align: left;
}
.pra-content.wrap>p>span{
	font-size: 0.5rem;
    line-height: 0.8rem;
}
.sub-btn{
	height: 1.75rem;
}
.praxis.wrap{
	padding-bottom: 1.65rem;
}
.praxis.wrap>input{
	margin-right: 3px;
}
.chat-word{
	margin-bottom: 1.46rem;
	margin-top: 8.3rem;
}
.chat-word ul li{
	padding: 10px;
	border-bottom: 1px solid #e6e6e6;
}
.chat-word ul li .user-img{
	width: 0.75rem;
	height: 0.75rem;
	border-radius: 50%;
}
.chat-word ul li .user-name{
	font-size: 0.47rem;
    vertical-align: top;
    line-height: 0.75rem;
}
.chat-word ul li .time{
	font-size: 0.4rem;
	line-height: 0.75rem;
}
.chat-word ul li .pl{
	font-size: 0.47rem;
}
.fix-header{
	position: fixed;
	left: 0;
	top: 0;
}
.fix-video{
	position: fixed;
	left: 0;
	top: 1.23rem;
}
.fix-record{
	position: fixed;
	left: 0;
	top: 6.99rem;
}
