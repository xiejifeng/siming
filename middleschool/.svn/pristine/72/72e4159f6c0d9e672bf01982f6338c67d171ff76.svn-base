.alert-box{
	padding-top:3.73rem;
}
.wrap{
	width:8.43rem;
	margin:0 auto;
}
.win{
	width:100%;
	height:5.96rem;
	border:0.03rem solid #000;
	box-sizing: border-box;
	font-size:0.66rem;
	position:relative;
	z-index: 3;
	background-color: white;
/*	-webkit-transform-style:preserve-3d;
	-webkit-transform:perspective(800px)*/
}
.win-alert{
	height: 3.96rem;
}
.win-alert .word{
	margin-top: 1.3rem;
}
.wrapper-alert{
	padding-top: 4.73rem;
}
.word{
	margin-top:1.7rem;
	text-align: center;
}
.y{
	width:1.72rem;
	height:1.72rem;
	border-radius: 50% 50% 0 0;
	position:absolute;
	left:50%;
	margin-left:-0.86rem;
	margin-top:-0.86rem;
	border:0.03rem solid #000;
	border-bottom: none;
	border-right:none;
	border-left: none;
	z-index: 2;
	background:url(../images/y.png) no-repeat center;
	background-size: 90% 90%;
	background-color:white;
	/*-webkit-transform:translateZ(-2px);*/
}
.close{
	width:0.37rem;
	position:absolute;
	right:0.21rem;
	top:0.21rem;
	height:0.37rem;
	background: url(../images/close.png) no-repeat;
	background-size: 100% 100%;
}
.reg,.login, .qx, .qd{
	position:absolute;
	bottom:0;
	height:1.2rem;
	width:50%;
}
.reg{
	left:0;
	background:url(../images/mfzc.png) no-repeat;
	background-size: 100% 100%;
}
.qx{
	left:0;
	background:url(../images/qx.png) no-repeat;
	background-size: 100% 100%;
}
.login{
	right:0;
	background:url(../images/ljdl.png) no-repeat;
	background-size: 100% 100%;
}
.qd{
	right:0;
	background:url(../images/qd.png) no-repeat;
	background-size: 100% 100%;
}
.alert-box{
	width: 100%;
	height: 100%;
	position: fixed;
	left: 0;
	top: 0;
	background: rgba(0, 0, 0, 0.65);
	z-index: 1000;
}
