 <sql-query name="SQL_All_11">
     <![CDATA[
SELECT
	t_all.*,t_all.accuracy as id, 
    IFNULL(t_week.week_accuracy,0)as week_accuracy,
	IFNULL(t_week.total,0) AS week_total,
	IFNULL(t_week.righttotal,0) AS week_righttotal
FROM
	(
		SELECT
			c.*, ue.tel,
			ue.`name`,
			ue.nickname,
			LEFT (
				(c.righttotal / c.total) * 100,
				5
			) AS accuracy
		FROM
			(
				SELECT
					a.*, b.righttotal
				FROM
					(
						SELECT
							re.user_id,
							COUNT(re.user_id) AS total
						FROM
							report re
						WHERE
							1 = 1
						GROUP BY
							re.user_id
					) a,
					(
						SELECT
							COUNT(a.user_id) AS righttotal,
							a.user_id
						FROM
							(
								SELECT
									*
								FROM
									report re
								WHERE
									1 = 1
								AND re.`status` = '1D00'
							) a
						WHERE
							1 = 1
						GROUP BY
							a.user_id
					) b
				WHERE
					1 = 1
				AND a.user_id = b.user_id
			) c,
			user_expand ue
		WHERE
			1 = 1
		AND ue.userId = c.user_id
		ORDER BY
			accuracy DESC
	) t_all LEFT JOIN
	(
		SELECT
			c.*, LEFT (
				(c.righttotal / c.total) * 100,
				5
			) AS week_accuracy
		FROM
			(
				SELECT
					a.*, b.righttotal
				FROM
					(
						SELECT
							COUNT(a.user_id) AS total,
							a.user_id
						FROM
							(
								SELECT
									*
								FROM
									report re
								WHERE
									1 = 1
								AND re.createtime > :startTime
								AND re.createtime < :endTime
							) a
						WHERE
							1 = 1
						GROUP BY
							a.user_id
					) a,
					(
						SELECT
							COUNT(a.user_id) AS righttotal,
							a.user_id
						FROM
							(
								SELECT
									*
								FROM
									report re
								WHERE
									1 = 1
								AND re.`status` = '1D00'
								AND re.createtime > :startTime
								AND re.createtime < :endTime
							) a
						WHERE
							1 = 1
						GROUP BY
							a.user_id
					) b
				WHERE
					1 = 1
				AND a.user_id = b.user_id
			) c,
			user_expand ue
		WHERE
			1 = 1
		AND ue.userId = c.user_id
		ORDER BY
			week_accuracy DESC
	) t_week ON
	1 = 1
AND t_all.user_id = t_week.user_id
/**<#if (tel??) && (tel?trim != "")>**/
 		   and t_all.tel like  :tel
/**</#if>**/
ORDER BY t_all.accuracy+0 DESC
     ]]>
 </sql-query>
 
