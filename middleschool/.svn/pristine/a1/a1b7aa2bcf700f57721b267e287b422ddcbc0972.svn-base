header{
  height:1.13rem;
  background-color:#00acf0;
  text-align: center;
  padding-top:0.6rem;
  position:relative;
}
input,select,textarea{
	border:1px solid #000;
	font-size: 0.55rem;
}
input,select{
	text-indent:0.5em;
}
textarea{
	width:100%;
	height:3rem;
	border-radius: 0.125rem;
	border:1px solid #000;
	padding:0.3rem;
	overflow-y: scroll;
	-webkit-box-sizing:border-box;
}
h2{
  font-size:0.61rem;
  color:white;
  font-weight: normal;
}
.back{
	position:absolute;
	left:0.29rem;
	color:white;
	bottom:0.1rem;
	font-family: 宋体;
}
.user-reg{
	padding-top:0.83rem;
	width:8.81rem;
	margin:0 auto;
}
.user-reg span{
	color:red;
	position:absolute;
	left:0;
	top:-0.7rem;
	font-size:0.5rem;
}
.user-reg .dp{
	width:2.12rem;
	margin:0 auto;
	margin-bottom:0.4rem;
}
.user-reg .dp a{
	display: block;
	width:100%;
	height:100%;
	font-size:0.34rem;
	text-align: center;
	color:#24aadf;
	text-decoration: underline;
}
.user-reg .dp p{
	margin-top:0.15rem;
}
.user-reg .dp img{
	width:100%;
	height:100%;
}
label{
	font-size:0.5rem;
	width:2.5rem;
	text-align: right;
}
#realname{
	height:1.2rem;
	border-radius: 0.125rem;
	width:calc(100% - 2.57rem);
}
.btn-wrap{
	position:fixed;
	height:2.65rem;
	left:0;
	bottom:0;
	background-color: #fff;
	text-align: center;
}
.btn-pro{
	width:8.8rem;;
	background-color: #00acf0;
	height:1.4rem;
	line-height:1.4rem;
	border: none;
	font-size:0.6rem;
	color:white;
	border-radius: 0.18rem;
}
.form-group{
	margin-bottom: 0.4rem;
	height:1.2rem;
	line-height: 1.2rem;
	position:relative;
}
.txt{
	border-radius: 0.125rem;
	border:1px solid #000;
	height:calc(100% - 0.06rem);
	width:calc(100% - 2.56rem);
}
select{
	width:100%;
	height:1.2rem;
	border-radius: 0.125rem;
}

.code{
	width:2rem;
	height:1.18rem;
	line-height:1.18rem;
	text-align: center;
	font-size:0.5rem;
	color:white;
	background-color: #00acf0;
	margin-left:0.29rem;
	border-radius:0.125rem;
}
.code-text{
	width:calc(100% - 4.88rem);
}
.logo-icon{
	text-align: center;
	margin-top:1rem;
}
.logo-icon img{
	width:4.5rem;
}
p.agree{
	font-size:0.375rem;
	padding-left:0.75rem;
	background: url(../images/check.png) no-repeat left;
}
#usercall{
	width:calc(100% - 2.56rem);
}