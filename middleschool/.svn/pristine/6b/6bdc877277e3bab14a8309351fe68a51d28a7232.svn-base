	<sql-query name="SQL_App_17">
    <![CDATA[
SELECT
		c.*, LEFT (
			(c.righttotal / c.total) * 100,
			5
		) AS week_accuracy
	FROM
		(
			SELECT
				a.*, b.righttotal
			FROM
				(
					SELECT
						COUNT(a.user_id) AS total,
						a.user_id
					FROM
						(
							SELECT
								*
							FROM
								report re
							WHERE
								1 = 1
							AND re.createtime > ?
							AND re.createtime < ?
						) a
					WHERE
						1 = 1
					GROUP BY
						a.user_id
				) a,
				(
					SELECT
						COUNT(a.user_id) AS righttotal,
						a.user_id
					FROM
						(
							SELECT
								*
							FROM
								report re
							WHERE
								1 = 1
							AND re.`status` = '1D00'
							AND re.createtime > ?
							AND re.createtime < ?
						) a
					WHERE
						1 = 1
					GROUP BY
						a.user_id
				) b
			WHERE
				1 = 1
			AND a.user_id = b.user_id
		) c,
		user_expand ue
	WHERE
		1 = 1
	AND ue.userId = c.user_id
	ORDER BY
		week_accuracy DESC,righttotal DESC
    ]]>
</sql-query>


