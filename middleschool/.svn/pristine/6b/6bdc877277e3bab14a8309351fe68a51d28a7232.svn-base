<hibernate-mapping>
	<sql-query name="SQL_App_1">
     <![CDATA[
		SELECT
			ue.*,
			su.username,
			su.`password`,
			su.account_non_expired,
			su.account_non_locked,
			su.credentials_non_expired
		FROM
			sys_user su,
			user_expand ue
		WHERE
			1 = 1
		AND su.id = ue.userId
		AND su.account_non_expired = 0
		AND (
			su.username = ?
			OR ue.tel = ?
		)     
     ]]>
	</sql-query>
	
