  float: left;
  position: relative;
}
.g-curriculumlist-box .g-list-box li .g-curriculumlist-img-box img {
  width: 92%;
  height: 0.7rem;
  position: absolute;
  left: 0;
  top: 0;
}
.g-curriculumlist-box .g-list-box li .g-curriculumlist-info-box {
  width: 65%;
  height: 0.7rem;
  display: inline-block;
  float: left;
  position: relative;
}
.g-curriculumlist-box .g-list-box li .g-curriculumlist-info-box span {
  font-size: 0.12rem;
  position: absolute;
}
.g-curriculumlist-box .g-list-box li .g-curriculumlist-info-box .cur-name {
  font-size: 0.14rem;
  top: 0.05rem;
  left: 0;
  width: 1.05rem;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.g-curriculumlist-box .g-list-box li .g-curriculumlist-info-box .cur-tec {
  top: 0.23rem;
  left: 0;
  color: #979696;
}
.g-curriculumlist-box .g-list-box li .g-curriculumlist-info-box .cur-date {
  bottom: 0.05rem;
  left: 0;
  color: #979696;
}
.g-curriculumlist-box .g-list-box li .g-curriculumlist-info-box .cur-tatus {
  top: 0.05rem;
  right: 0;
  color: #feb801;
}
.g-curriculumlist-box .g-list-box li.active {
  background: #eee;
  width: 100%;
  margin-left: -0.2rem;
  padding: 0.1rem 0.2rem;
}
.g-form-group-box {
  width: 100%;
  padding-top: 0rem;
  margin-bottom: 0.6rem;
}
.g-form-group-box .about-school-box {
