  font-size: 0.16rem;
  font-weight: normal;
}
.g-curriculumlist-box .g-list-title-box h2 span {
  line-height: 0.4rem;
  font-size: 0.12rem;
  padding: 0 0.2rem;
  color: #feb801;
  float: right;
  text-decoration: underline;
}
.g-curriculumlist-box .g-list-box {
  width: 100%;
  height: auto;
  padding: 0 0.2rem 0.2rem;
}
.g-curriculumlist-box .g-list-box li {
  width: 100%;
  height: 0.9rem;
  border-bottom: 1px solid #eee;
  padding: 0.1rem 0;
