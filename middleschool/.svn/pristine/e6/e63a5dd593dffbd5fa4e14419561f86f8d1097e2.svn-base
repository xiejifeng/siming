  color: #fff;
  background: #feb801;
  float: right;
}
.g-form-group-box .g-form-group .form-list .provision-box {
  border: none;
  text-align: left;
  width: 100%;
  font-size: 0.13rem;
}
.g-form-group-box .g-form-group .form-list .provision-box input {
  width: 0.15rem;
  height: 0.15rem;
  float: left;
}
.g-form-group-box .g-form-group .form-list .provision-box span {
  float: left;
  padding-left: 0.06rem;
}
.g-form-group-box .g-form-group .form-list .provision-box a {
  color: #feb801;
  text-decoration: none;
  padding-left: 0.06rem;
}
.g-form-group-box .g-form-group .form-box {
  width: 100%;
  padding: 0.15rem 0.2rem;
  margin-bottom: 0.06rem;
  position: relative;
}
.g-form-group-box .g-form-group .form-box .agreement-box {
  width: 100%;
  height: 0.5rem;
