  position: absolute;
  top: 1.35rem;
  text-decoration: none;
}
.g-login-box .g-submite-box .forget-password {
  left: 0.2rem;
}
.g-login-box .g-submite-box .register {
  right: 0.2rem;
}
.g-tab-box {
