  line-height: 0.16rem;
  float: right;
  font-size: 0.12rem;
}
.g-form-group-box .g-form-group .form-list .g-input-btn-box {
  width: 74%;
  height: 0.3rem;
  display: inline-block;
  float: right;
}
.g-form-group-box .g-form-group .form-list .g-input-btn-box input {
  width: 62%;
  height: 0.3rem;
