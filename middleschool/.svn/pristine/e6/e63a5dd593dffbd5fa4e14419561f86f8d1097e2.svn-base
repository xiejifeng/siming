html {
  font-size: 100px;
}
body {
  background: #f5f5f5;
  font-size: 0.13rem;
}
ul {
  list-style-type: none;
}
.dis-none {
  display: none !important;
}
.dis_none {
  display: none !important;
}
.f-l {
  float: left !important;
}
a:focus,
input:focus,
select:focus,
button:focus,
textarea:focus {
  outline: none;
}
* {
  margin: 0;
  padding: 0;
  font: 0.12rem;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
.title-box {
  width: 100%;
  height: 0.44rem;
  background: #feb801;
}
.title-box .title {
  color: #3b3b3b;
  text-align: center;
  font-size: 0.14rem;
  line-height: 0.44rem;
}
.title-box .g-left-icon {
  height: 0.44rem;
  width: 0.6rem;
  padding: 0.08rem 0.13rem;
  position: absolute;
  left: 0;
  top: 0;
}
.title-box .g-left-icon img {
  position: absolute;
  left: 0.13rem;
  top: 0.17rem;
  height: 0.23rem;
}
.title-box .g-left-icon.active {
  background: #eee;
  border-bottom: 1px solid #feb801;
}
.title-box .g-right-icon {
  width: 0.8rem;
  height: 0.44rem;
  padding: 0.09rem;
  position: absolute;
  right: 0;
  top: 0;
}
.title-box .g-right-icon .sign-btn {
  width: 0.61rem;
  height: 0.26rem;
  line-height: 0.26rem;
  text-align: center;
  border: none;
  background: none;
  position: absolute;
  top: 0.09rem;
  right: 0.09rem;
  color: #feb801;
  font-size: 0.13rem;
  text-align: right;
  padding-right: 0.1rem;
}
.title-box .g-right-icon .sign-btn img {
  width: 0.26rem;
  height: 0.26rem;
}
.g-banner-box {
  width: 100%;
  height: 1.33rem;
  position: relative;
  background: #feb801;
  overflow: hidden;
}
.g-banner-box ul {
  list-style: none;
  margin: 0;
  padding: 0;
  height: 100%;
  overflow: hidden;
}
.g-banner-box ul li {
  position: absolute;
}
.g-banner-box .banner-list {
  width: 100%;
  height: 1.6rem;
}
.g-banner-box .banner-list li {
  width: 100%;
  height: 1.6rem;
  overflow: hidden;
}
.g-banner-box .banner-list li img {
  width: 100%;
}
.g-entrance-box {
  width: 100%;
  height: 0.8rem;
  overflow: hidden;
  background: #feb801;
}
.g-entrance-box ul {
  display: -webkit-box;
}
.g-entrance-box .g-entrance {
  -webkit-box-flex: 1;
  height: 0.5rem;
  margin: 0.15rem;
  background: #feb801;
  display: inline-block;
  float: left;
  position: relative;
}
.g-entrance-box .g-entrance .icon {
  width: 0.34rem;
  height: 0.34rem;
  position: absolute;
  top: 0rem;
  left: 0.06rem;
}
.g-entrance-box .g-entrance .icon-title {
  color: #fff;
  text-align: center;
  font-size: 0.12rem;
  width: 0.5rem;
  position: absolute;
  left: 0;
  bottom: 0.03rem;
}
.g-entrance-box li.active {
  opacity: 0.5;
}
.g-curriculumlist-box {
  width: 100%;
  height: auto;
  margin-top: 0.1rem;
  background: #fff;
}
.g-curriculumlist-box .g-list-title-box {
  width: 100%;
  height: 0.39rem;
  border-top: 1px solid #eee;
}
.g-curriculumlist-box .g-list-title-box h2 {
  width: 100%;
  padding-left: 0.2rem;
  line-height: 0.4rem;
  color: #3b3b3b;
